        "def trend(time,slope=0):\n",
        "  series=slope*time\n",
        "  return series\n",
        "\n",
        "time=np.arange(365)\n",
        "slope=-0.01\n",
        "series=trend(time,slope)\n",
        "plot_series(time,series,label=[f'slope={slope}'])"
      ],
