    {
      "cell_type": "code",
      "source": [
        "def autocorrelation(time,amplitude,seed=None):\n",
        "  rnd=np.random.RandomState(seed)\n",
        "  ar=rnd.randn(len(time)+1)\n",
        "  phi=0.8\n",
        "  for step in range(1,len(time)+1):\n",
        "    ar[step]+=phi*ar[step-1]\n",
        "  ar=ar[1:]*amplitude\n",
        "  return ar"
      ],
      "metadata": {
        "id": "nYO3WsVYcDKw"
      },
      "execution_count": 30,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
